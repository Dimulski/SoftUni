@model FDMC.Models.ViewModels.CatDetailsViewModel
@{
    ViewData["Title"] = "Cat Details";
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-5">
        <h1>@Model.Name</h1>
        <img src="@Model.ImageUrl" alt="@Model.Name" class="img-fluid" style="max-width: 300px;"/>
        <h4>Age: @Model.Age</h4>
        <h4>Breed: @Model.Breed</h4>
    </div>
</div>